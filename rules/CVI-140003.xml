<?xml version="1.0" encoding="UTF-8"?>

<cobra document="https://github.com/wufeifei/cobra">
    <name value="Print Param"/>
    <language value="php"/>
    <match><![CDATA[((echo(\s*\()?|(print(_r)?|exit|die|printf|vprintf|trigger_error|user_error|odbc_result_all|ovrimos_result_all|ifx_htmltbl_result)\s*\()\s*[^;]*\$_(GET|POST|REQUEST|SERVER|COOKIE))]]></match>
    <solution>
        输出入参会导致XSS
    </solution>
    <test>
        <case assert="true"><![CDATA[echo ($_GET['test']);]]></case>
        <case assert="true"><![CDATA[echo $_GET['test'];]]></case>
    </test>
    <status value="on"/>
    <author name="Feei" email="feei@feei.cn"/>
</cobra>